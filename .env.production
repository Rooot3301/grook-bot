# Configuration optimisée pour la production
DISCORD_TOKEN=VotreTokenDiscordIci
CLIENT_ID=VotreClientIDIci

# Désactiver les logs de debug en production
LOG_LEVEL=info
NODE_ENV=production

# Optimisations performance
LINK_GUARDIAN_ENABLED=true
LINK_HEAD_CHECK=false
LINK_RISK_THRESHOLD=4

# Désactiver les notifications webhook en prod si pas nécessaire
SKIP_WEBHOOK=false
WEBHOOK_URL=VotreWebhookURL

# LLM désactivé par défaut (économise les ressources)
LLM_ON_MENTION=false

# Timezone
TZ=Europe/Paris